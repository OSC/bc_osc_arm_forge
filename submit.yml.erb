<%-
ppn = num_cores.blank? ? 28 : num_cores.to_i
if node_type == ':hugemem' && num_of_nodes.to_i > 16
  ppn = 48
  num_of_nodes = "16"
end
%>
---
batch_connect:
  template: vnc
script:
  native:
    resources:
      nodes: "<%= num_of_nodes.blank? ? "1" : num_of_nodes.to_i %>:ppn=<%= ppn %><%= node_type %>"
